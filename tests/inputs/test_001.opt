	JSR main
	HALT
main:	ADD R6, R6, #-1
	ADD R6, R6, #-1
	STR R7, R6, #0
	ADD R6, R6, #-1
	STR R5, R6, #0
	ADD R5, R6, #-1
	ADD R6, R6, #-1
	SET R8, #100
	ADD R9, R5, #0
	STR R8, R9, #0
	SET R10, #0
